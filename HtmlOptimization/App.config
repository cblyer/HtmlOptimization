<?xml version="1.0" encoding="utf-8" ?>
<configuration>

  <!-- Plugin configuration start -->
  <configSections>
    <section name="HtmlOptimization" type="HtmlOptimization.Config.Sections.ConfigSection, HtmlOptimization" allowDefinition="Everywhere" allowLocation="true"/>
  </configSections>

  <HtmlOptimization xmlns="urn:HtmlOptimization.Config">
    <CompressionModule compressionType="GZip">
      <Skip>
        <Extension value=".axd"/>
        <Extension value=".aspx"/>
      </Skip>
    </CompressionModule>
    <MinifyModule>
      <Skip>
        <Extension value=".axd"/>
        <Extension value=".aspx"/>
      </Skip>
    </MinifyModule>
  </HtmlOptimization>
  <!-- Plugin configuration end -->

  <appSettings>
  </appSettings>
  <system.webServer>
    <modules>
      <add name="MinifyModule" type="HtmlOptimization.Modules.MinifyModule, HtmlOptimization"/>
      <add name="CompressModule" type="HtmlOptimization.Modules.CompressionModule, HtmlOptimization"/>
    </modules>
  </system.webServer>
</configuration>